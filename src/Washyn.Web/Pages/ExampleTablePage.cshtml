<div class="card">
    <div class="card-body">
        @* can be set search button as submit and input event submit *@
        <form>
            <wsh-search-input value="@Model.Filter"/>
        </form>
        <br>
        
        <table class="table table-striped table-bordered">
            <thead class="rounded">
            <tr>
                <th>#</th>
                <th>Value</th>
            </tr>
            </thead>
            <tbody>
            @foreach (var dto in Model.List.Items)
            {
                <tr>
                    <td>@dto.Id</td>
                    <td>@dto.Nombre</td>
                </tr>
            }
            </tbody>
        </table>
        <abp-paginator model="Model.PagerModelInfo" show-info="true"/>
    </div>
</div>
<br>
