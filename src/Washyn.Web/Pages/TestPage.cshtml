@page
@model Washyn.Web.Pages.TestPage

@{
    // Layout = null;
}

<hr>

<h5 class="text-center">TimeZones for country</h5>

<ul>
    @foreach (var item in Model.TimeZones)
    {
        <li>
            @item.Key - @item.Value
        </li>
    }
</ul>

<h5 class="text-center">TimeZones ids </h5>
@foreach (var item in Model.TimeZoneIds)
{
    <li>
        @item
    </li>
}


<h2>Windows Format is : @Model.WindowsFormat</h2>

